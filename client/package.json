{
  "name": "clientnew",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "node-sass": "^8.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.4",
    "react-scripts": "^5.0.1",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
	"lint": "eslint .",
	"lint:fix": "eslint --fix ."
  },
  "eslintConfig": {
    "extends": "react-app",
	"ignorePatterns": ["**/node_modules/**/*", "**/build/**/*"],
	"rules": {
		"indent": [
			"error",
			4,
			{
				"SwitchCase": 1,
				"VariableDeclarator": 1,
				"outerIIFEBody": 1,
				"FunctionDeclaration": {
				"parameters": 1,
				"body": 1
				},
				"FunctionExpression": {
				"parameters": 1,
				"body": 1
				},
				"CallExpression": {
				"arguments": 1
				},
				"ArrayExpression": 1,
				"ObjectExpression": 1,
				"ImportDeclaration": 1,
				"flatTernaryExpressions": false,
				"ignoredNodes": [
					"JSXElement",
					"JSXElement > *",
					"JSXAttribute",
					"JSXIdentifier",
					"JSXNamespacedName",
					"JSXMemberExpression",
					"JSXSpreadAttribute",
					"JSXExpressionContainer",
					"JSXOpeningElement",
					"JSXClosingElement",
					"JSXFragment",
					"JSXOpeningFragment",
					"JSXClosingFragment",
					"JSXText",
					"JSXEmptyExpression",
					"JSXSpreadChild"
				],
				"ignoreComments": false
			}
		],
		"no-mixed-spaces-and-tabs": ["error"],
		"quotes": ["error", "single"],
		"max-len": [
			"error",
			{
				"code": 130,
				"comments": 130,
				"ignoreTrailingComments": true,
				"ignoreUrls": true,
				"ignoreRegExpLiterals": true
			}
		]
	  }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
